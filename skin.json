{
	"name": "ForTrainingNG",
	"version": "1.0",
	"author": "...",
	"url": "https://github.com/4training/mediawiki-skins-ForTrainingNG",
	"descriptionmsg": "fortrainingng-desc",
	"namemsg": "skinname-fortrainingng",
	"license-name": "CC0-1.0",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ValidSkinNames": {
		"fortrainingng": {
			"class": "SkinMustache",
			"args": [ {
				"name": "fortrainingng",
				"responsive": true,
				"templateDirectory": "skins/ForTrainingNG/templates",
				"styles": [
					"skins.fortrainingng"
				],
				"messages": [
					"sitetitle",
					"search",
					"tagline",
					"navigation-heading"
				],
				"scripts": [
					"skins.fortraining.js"
				]
			} ]
		}
	},
	"MessagesDirs": {
		"ForTrainingNG": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.fortrainingng": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"normalize": true,
				"elements": true,
				"content": true,
				"interface": true,
				"logo": true
			},
			"styles": {
				"resources/screen-common.less": {
					"media": "screen"
				},
				"resources/screen-desktop.less": {
					"media": "screen and (min-width: 751px)"
				},
				"resources/screen-mobile.less": {
					"media": "screen and (max-width: 750px)"
				},
				"resources/print.css": {
					"media": "print"
				}
			}
		},
		"skins.fortrainingng.js": {
			"scripts": [
				"resources/main.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "ForTrainingNG"
	},
	"ResourceModuleSkinStyles": {
		"fortrainingng": {
			"+ext.echo.styles.badge": "resources/extensions/echo.styles.badge.less"
		}
	},
	"manifest_version": 1
}
